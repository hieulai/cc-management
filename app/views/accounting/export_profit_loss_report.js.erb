<% if flash[:notice].present? %>
$.rails.createConfirmDialog('Information', '<%= escape_javascript(flash[:notice].html_safe) %>', true);
<% else
   options = {from_date: @from_date, to_date: @to_date, project_id: @project_id }
%>

$(".pl-report-placeholder").html('<%= escape_javascript(render(:partial => "profit_loss_report", :locals => {options: options})) %>');
$('tr[class^=account_]').hide();
$('.expander').simpleexpand({throwOnMissingTarget: false });
<% end %>
