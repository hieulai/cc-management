<% if flash[:notice].present? %>
$.rails.createConfirmDialog('Information', '<%= escape_javascript(flash[:notice].html_safe) %>', true);
<% else %>
$(".pl-report-placeholder").html('<%= escape_javascript(render(:partial => "profit_loss_report", :locals => {from_date: @from_date, to_date: @to_date })) %>');
<% end %>
