<div>
    <%= render(:partial => "layouts/accounting_sub_menu") %>
    <h2>Add Supplier</h2>

    <%= form_for @account, :url => {:action => 'create'}, html: {class: 'form-horizontal'} do |f| %>

      <%= render(:partial => "new_form", :locals => {:f => f}) %>

      <div class="control-group">
          <div class='controls'>
              <%= submit_tag("Create", class: 'btn') %>
          </div>
      </div>


    <% end %>

</div>