<tr class="nested-fields co-item">
    <td></td>
    <td>
        <%= autocomplete_field :name, nil, items_autocomplete_name_path, :class => 'co-item-name', :value => f.object.name %>
        <% if f.object.id %>
          <%= f.hidden_field :id, :value => f.object.id %>
        <% else %>
          <%= f.hidden_field :id, :class => 'id' %>
        <% end %>

        <%= f.hidden_field :name, :class => 'name' %>
        <%= f.hidden_field :unit, :class => 'unit' %>
    </td>
    <td>
        <%= f.text_field :description, :class => 'description' %>
    </td>
    <td>
        <%= f.text_field :qty, :class => 'input-mini co-factor qty' %>
    </td>
    <td>
        $<%= f.text_field :estimated_cost, :class => 'input-mini co-factor estimated_cost' %>
    </td>
    <td>
        $<%= f.text_field :margin, :class => 'input-mini co-factor margin' %>
    </td>
    <td class="item-total right"></td>
    <td>
        <%= link_to_remove_association("Delete", f, style: "margin-left: 30px") %>
    </td>
</tr>