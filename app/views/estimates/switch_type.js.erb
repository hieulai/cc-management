var $templatePlaceholder = $('#template-placeholder');
$templatePlaceholder.attr("data-kind", "<%= @estimate.kind %>")
<% if @estimate.kind == "Cost Plus Bid" %>
$templatePlaceholder.html('<%= escape_javascript(render :partial =>  'estimates/cost_plus_bid/template') %>');
<% else %>
$templatePlaceholder.html('<%= escape_javascript(render :partial =>  'template') %>');
<% end %>
Shared.calculateTotals();
Shared.calculateSubTotals();
Estimate.init();
