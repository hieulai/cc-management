<div>
    <%= render(:partial => "layouts/project_mgmt_sub_menu") %>
    <h2>Edit Specification</h2>

    <%= form_for @specification, :method => :put, html: {class: 'form-horizontal'} do |f| %>
      <%= render(:partial => "form", :locals => {:f => f}) %>

      <div class="control-group">
          <div class='controls'>
              <%= submit_tag("Update", class: 'btn') %>
          </div>
      </div>


    <% end %>

</div>